{
  "name": "electron-chromedriver",
  "version": "0.0.0-development",
  "description": "Electron ChromeDriver",
  "repository": "https://github.com/electron/chromedriver",
  "bin": {
    "chromedriver": "./chromedriver.js"
  },
  "files": [
    "./chromedriver.js",
    "./download-chromedriver.js"
