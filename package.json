    "@electron/get": "^2.0.1",
    "extract-zip": "^2.0.0"
  },
  "devDependencies": {
    "@electron/fiddle-core": "^1.3.3",
    "mocha": "^10.1.0",
    "prettier": "^3.6.2"
  }
}
